<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:background="@android:color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <FrameLayout
            android:layout_width="378dp"
            android:layout_height="44dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp">


            <FrameLayout
                android:layout_width="200dp"
                android:layout_height="42dp"
                android:layout_marginRight="94dp"
                android:layout_marginLeft="96dp"
                android:layout_marginTop="1dp">

                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="42dp"
                    android:layout_marginLeft="45dp"
                    android:layout_marginRight="50dp"
                    android:background="@drawable/raplpng">

                </ImageView>


            </FrameLayout>
            <FrameLayout
                android:layout_width="42dp"
                android:layout_height="42dp"
                android:layout_marginTop="1dp">
                <ImageView
                    android:layout_width="42dp"
                    android:layout_height="42dp"
                    android:background="@drawable/circle">
                </ImageView>
                <ImageView
                    android:id="@+id/clickable_arrow"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginLeft="9dp"
                    android:background="@drawable/arrow1">

                </ImageView>


            </FrameLayout>

        </FrameLayout>

    </com.google.android.material.appbar.AppBarLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="-33dp"
        tools:layout_editor_absoluteY="-231dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:id="@+id/linear1"
                android:layout_width="328dp"
                android:layout_height="671dp"
                android:background="@drawable/shape"
                android:layout_marginTop="108dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="24dp"
                    android:text="My Profile"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="16dp"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold">

                </TextView>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="20dp"
                    android:orientation="vertical"
                    android:paddingVertical="16dp">

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="304dp"
                android:layout_height="610dp"
                android:layout_marginTop="47dp"
                android:background="@drawable/shape1"
                app:layout_constraintEnd_toEndOf="@+id/linear1"
                app:layout_constraintStart_toStartOf="@+id/linear1"
                app:layout_constraintTop_toTopOf="@+id/linear1">

                <ImageView
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginLeft="91dp"
                    android:background="@drawable/profile">

                </ImageView>

                <ImageView
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    android:layout_marginTop="96dp"
                    android:layout_marginLeft="-38dp"
                    android:background="@drawable/shape2">

                </ImageView>

                <ImageView
                    android:id="@+id/camera_1"
                    android:layout_width="16.67dp"
                    android:layout_height="15dp"
                    android:background="@drawable/cam"
                    android:layout_marginTop="105dp"
                    android:layout_marginLeft="-27dp">

                </ImageView>

                <TextView
                    android:id="@+id/first_name"
                    android:layout_width="86dp"
                    android:layout_height="19dp"
                    android:layout_marginTop="167dp"
                    android:layout_marginLeft="-171dp"
                    android:text="First Name"

                    >

                </TextView>

                <TextView
                    android:id="@+id/name"
                    android:layout_width="256dp"
                    android:layout_height="21dp"
                    android:layout_marginTop="190dp"
                    android:layout_marginLeft="-85dp"
                    android:text="Admin"
                    android:textSize="18dp"
                    android:textColor="@color/black">
                </TextView>
                
                <ImageView
                    android:id="@+id/edit1"
                    android:layout_width="16.95dp"
                    android:layout_height="16.93dp"
                    android:layout_marginLeft="-10dp"
                    android:background="@drawable/edit"
                    android:layout_marginTop="190.57dp">

                </ImageView>

                <TextView
                    android:id="@+id/lst_name"
                    android:layout_width="85dp"
                    android:layout_height="19dp"
                    android:layout_marginTop="229dp"
                    android:layout_marginLeft="-261dp"
                    android:text="Last Name"

                    >

                </TextView>

                <TextView
                    android:id="@+id/last_name"
                    android:layout_width="256dp"
                    android:layout_height="21dp"
                    android:layout_marginTop="252dp"
                    android:layout_marginLeft="-85dp"
                    android:text="Admin Admin Admin..."
                    android:textSize="18dp"
                    android:textColor="@color/black">
                </TextView>

                <ImageView
                    android:id="@+id/edit2"
                    android:layout_width="16.95dp"
                    android:layout_height="16.93dp"
                    android:layout_marginLeft="-10dp"
                    android:background="@drawable/edit"
                    android:layout_marginTop="252.57dp">

                </ImageView>

                <TextView
                    android:id="@+id/phone_no"
                    android:layout_width="49dp"
                    android:layout_height="19dp"
                    android:layout_marginTop="291dp"
                    android:layout_marginLeft="-261dp"
                    android:text="Phone"/>

                <TextView
                    android:id="@+id/number"
                    android:layout_width="256dp"
                    android:layout_height="21dp"
                    android:layout_marginTop="314dp"
                    android:layout_marginLeft="-48dp"
                    android:text="+91 85342100 "
                    android:textSize="18dp"
                    android:textColor="@color/black"/>

                <ImageView
                    android:id="@+id/edit3"
                    android:layout_width="16.95dp"
                    android:layout_height="16.93dp"
                    android:layout_marginLeft="-10dp"
                    android:background="@drawable/edit"
                    android:layout_marginTop="314.57dp"/>


                <TextView
                    android:id="@+id/mail"
                    android:layout_width="32dp"
                    android:layout_height="19dp"
                    android:layout_marginTop="353dp"
                    android:layout_marginLeft="-261dp"
                    android:text="Mail"/>

                <TextView
                    android:id="@+id/id"
                    android:layout_width="256dp"
                    android:layout_height="21dp"
                    android:layout_marginTop="376dp"
                    android:layout_marginLeft="-48dp"
                    android:text="admin@getrapl.com"
                    android:textSize="18dp"
                    android:textColor="@color/black"/>

                <ImageView
                    android:id="@+id/edit4"
                    android:layout_width="16.95dp"
                    android:layout_height="16.93dp"
                    android:layout_marginLeft="-10dp"
                    android:background="@drawable/edit"
                    android:layout_marginTop="376.57dp"/>

                <TextView
                    android:id="@+id/password"
                    android:layout_width="49dp"
                    android:layout_height="19dp"
                    android:layout_marginTop="415dp"
                    android:layout_marginLeft="-261dp"
                    android:text="Mail"/>

                <TextView
                    android:id="@+id/hidden"
                    android:layout_width="256dp"
                    android:layout_height="21dp"
                    android:layout_marginTop="438dp"
                    android:layout_marginLeft="-48dp"
                    android:text="admin@getrapl.com"
                    android:textSize="18dp"
                    android:textColor="@color/black"/>

                <ImageView
                    android:id="@+id/edit5"
                    android:layout_width="16.95dp"
                    android:layout_height="16.93dp"
                    android:layout_marginLeft="-10dp"
                    android:background="@drawable/edit"
                    android:layout_marginTop="438.57dp"/>












            </LinearLayout>







        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>

